{
  "manifest_version": 3,
  "name": "Coralogix Customer Banner",
  "version": "1.0.0",
  "description": "Displays a prominent banner showing the Coralogix customer domain to help users clearly identify the environment",
  "author": "bit-cloner",
  "homepage_url": "https://github.com/bit-cloner/coralogix-customer-banner",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.coralogix.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": []
}